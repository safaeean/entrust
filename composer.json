    "require": {
        "php": "*",
        "illuminate/console": "~7.0|~8.0",
        "illuminate/support": "~7.0|~8.0",
        "illuminate/cache": "~7.0|~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "mockery/mockery": "dev-master",
        "illuminate/database": "~7.0"
